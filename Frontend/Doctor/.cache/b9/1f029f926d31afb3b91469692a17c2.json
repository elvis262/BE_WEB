onsole.log('first ',editor)\r\n    return editor\r\n}\r\nexport function hideDivWithOpacityAnimation() {\r\n    const target = modal.querySelector('.content')\r\n\r\n    setTimeout(function () {\r\n        target.style.opacity = '0'\r\n    }, 30); // Temps correspondant à la durée de transition CSS\r\n    setTimeout(()=>{\r\n        target.remove()\r\n        modal.style.opacity = '0'\r\n    },60)\r\n    setTimeout(()=>{\r\n        modal.style.display = 'none';\r\n    },110)\r\n\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"c44aef2482a095047e19cedba5e711b7","cacheData":{"env":{}}}